<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />

    <package name="default" namespace="/" extends="struts-default">       

        <action name="addGuitar" class="guitar.Action.AddAction">
			<result>/WEB-INF/content/success.jsp</result>
		</action>
		<action name="searchGuitar" class="guitar.Action.SearchAction">
			<result>/WEB-INF/content/list.jsp</result>
		</action>
		<action name="GetAll" class="guitar.Action.GetAllAction">
			<result>/WEB-INF/content/list.jsp</result>
		</action>
    </package>


   

</struts>